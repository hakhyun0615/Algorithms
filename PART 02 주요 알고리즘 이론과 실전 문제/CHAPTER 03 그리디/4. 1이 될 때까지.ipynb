   "source": [
    "n, k = map(int, input().split()) \n",
    "\n",
    "num = 0\n",
    "\n",
    "while True:\n",
    "    if n == 1:\n",
    "        break\n",
    "    if n % k == 0:\n",
    "        n /= k\n",
    "    else:\n",
    "        n -= 1\n",
    "    num += 1\n",
    "\n",
    "print(num)"
   ]
